{
  "name": "latex-image-paste",
  "main": "./lib/main",
  "version": "0.4.0",
  "description": "When an image is pasted into LaTeX, this package creates a directory including the image and inserts a figure including the image path into the document.",
  "keywords": [
    "latex",
    "image",
    "paste",
    "figure",
    "clipboard",
    "copy"
  ],
  "activationHooks": [
    "language-latex:grammar-used",
    "language-tex:grammar-used",
    "language-latexsimple:grammar-used",
    "language-latex2e:grammar-used"
  ],
  "repository": "https://github.com/simon123h/latex-image-paste",
  "license": "MIT",
  "engines": {
    "atom": ">=1.0.0 <2.0.0"
  }
}
